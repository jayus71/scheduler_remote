# Kubernetes配置
KUBERNETES_SERVICE_HOST=kubernetes.default.svc
KUBERNETES_SERVICE_PORT=443
# 是否使用ServiceAccount配置
USE_SERVICE_ACCOUNT=false
KUBE_NAMESPACE=test-scheduler


# 应用配置
APP_NAME=edge-scheduler
APP_VERSION=1.0.0
DEBUG=True
LOG_LEVEL=INFO

# API配置
API_V1_PREFIX=/v1
HOST="0.0.0.0"
PORT=8000

# Kubernetes资源限制
DEFAULT_CPU_REQUEST=100m
DEFAULT_MEMORY_REQUEST=128Mi
DEFAULT_CPU_LIMIT=200m
DEFAULT_MEMORY_LIMIT=256Mi

# 调度策略配置
SCHEDULER_FILTER_TIMEOUT=5
MAX_PODS_PER_NODE=110
ENABLE_GPU_SCHEDULING=false

# 优化器配置
ENABLE_OPTIMIZER=true
WEIGHT_CPU=0.6
WEIGHT_MEMORY=0.4
OPTIMIZER_ALGORITHM=default

# 遗传算法配置
GA_POPULATION_SIZE=50
GA_GENERATIONS=100
GA_MUTATION_RATE=0.1
GA_WEIGHT_CPU=0.5
GA_WEIGHT_MEMORY=0.3
GA_WEIGHT_BALANCE=0.2

# 强化学习配置
ENABLE_RL=true
RL_ALGORITHM=dqn
# 学习率(调整网络更新速度)
RL_LEARNING_RATE=0.001
# 探索率(调整探索与利用的平衡)
RL_GAMMA=0.99
RL_EPSILON=0.1
RL_EPSILON_MIN=0.01
RL_EPSILON_DECAY=0.995
# 批大小(调整每次训练的样本数量)
RL_BATCH_SIZE=32
# 经验回放缓冲区大小(存储历史经验)
RL_MEMORY_SIZE=10000
# 目标网络更新频率(控制更新速度)
RL_TARGET_UPDATE_FREQ=10
# 模型保存间隔
RL_MODEL_SAVE_INTERVAL=10
# 模型保存路径
RL_MODEL_PATH=models/rl
RL_HYBRIDIZATION_WEIGHT=0.5

# 算力量化配置
ENABLE_COMPUTING_POWER=true
ENABLE_COMPUTING_POWER_FILTER=true
COMPUTING_POWER_FALLBACK=true
# 确定性路由软件地址
COMPUTING_ROUTER_HOST=127.0.0.1
COMPUTING_ROUTER_PORT=8888

# 网络信息上报服务配置
UDP_HOST=0.0.0.0
UDP_PORT=9001
# 算力信息上报服务配置
UDP_SERVICE_HOST=0.0.0.0
UDP_SERVICE_PORT=9002
UDP_TIMEOUT=10
UDP_MAX_RETRIES=3
UDP_RETRY_DELAY=1.0
UDP_EXPONENTIAL_BACKOFF=true

# 算力权重配置
WEIGHT_CPU_POWER=0.5
WEIGHT_GPU_POWER=0.3
WEIGHT_FPGA_POWER=0.2

# 默认算力配置
DEFAULT_CPU_POWER_RATIO=0.8
DEFAULT_GPU_TFLOPS=5.0
DEFAULT_FPGA_TFLOPS=2.0


# PSO配置
PSO_SWARM_SIZE=30
PSO_MAX_ITERATIONS=50
PSO_INERTIA_WEIGHT=0.7
PSO_COGNITIVE_COEF=1.5
PSO_SOCIAL_COEF=1.5
PSO_WEIGHT_CPU=0.25
PSO_WEIGHT_MEMORY=0.25
PSO_WEIGHT_BALANCE=0.25
PSO_WEIGHT_NETWORK=0.25


# 优化方法选择
OPTIMIZATION_METHOD=aco


# 模拟退火算法(SA)配置
SA_INITIAL_TEMP=100.0
SA_COOLING_RATE=0.95
SA_MIN_TEMP=0.1
SA_ITERATIONS_PER_TEMP=10
SA_WEIGHT_CPU=0.25
SA_WEIGHT_MEMORY=0.25
SA_WEIGHT_BALANCE=0.25
SA_WEIGHT_NETWORK=0.25


# 蚁群算法(ACO)参数
ACO_ANTS_COUNT=20
ACO_ITERATIONS=50
ACO_ALPHA=1.0
ACO_BETA=2.0
ACO_RHO=0.5
ACO_Q=100.0
ACO_WEIGHT_CPU=0.25
ACO_WEIGHT_MEMORY=0.25
ACO_WEIGHT_BALANCE=0.25
ACO_WEIGHT_NETWORK=0.25



